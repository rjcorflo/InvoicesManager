<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="200" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="imecorpa.app.information.InformationController">
    <TableView fx:id="logInfo" maxWidth="1.7976931348623157E308" tableMenuButtonVisible="true">
        <columns>
            <TableColumn fx:id="logTypeColumn" maxWidth="500.0" minWidth="100.0" prefWidth="150.0" text="%view.information.type">
            </TableColumn>
            <TableColumn fx:id="logMessage" maxWidth="2000.0" minWidth="500.0" prefWidth="-1.0" text="%view.information.message">
                <cellValueFactory>
                    <PropertyValueFactory property="logMessage" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="logTime" maxWidth="800.0" minWidth="200.0" prefWidth="500.0" text="%view.information.time">
            </TableColumn>
        </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
</VBox>
