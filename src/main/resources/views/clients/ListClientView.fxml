<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="clientList" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="imecorpa.app.client.ListClientController">
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <GridPane HBox.hgrow="ALWAYS">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
         </GridPane>
         <ButtonBar prefHeight="40.0" prefWidth="200.0">
           <buttons>
               <Button layoutX="135.0" layoutY="45.0" mnemonicParsing="false" text="AÃ±adir" onAction="#addClient" />
             <Button mnemonicParsing="false" text="Edit" onAction="#editClient" />
           </buttons>
         </ButtonBar>
      </children>
   </HBox>
<TableView fx:id="tableClient" VBox.vgrow="ALWAYS">
    <columns>
        <TableColumn fx:id="tableColumnId" prefWidth="50" text="%default.id" />
        <TableColumn fx:id="tableColumnName" prefWidth="150" text="%default.name" />
        <TableColumn fx:id="tableColumnLastname" prefWidth="800.0" text="%default.lastname" />
        <TableColumn fx:id="tableColumnNif" prefWidth="150" text="%default.nif" />
        <TableColumn fx:id="tableColumnEmail" prefWidth="300" text="%default.email" />
    </columns>
</TableView>
</VBox>
