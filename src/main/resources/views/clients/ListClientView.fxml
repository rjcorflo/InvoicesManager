<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="clientList"  xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="imecorpa.app.client.ListClientController">
<TableView fx:id="tableClient">
    <columns>
        <TableColumn fx:id="tableColumnId" prefWidth="50" text="%default.id"/>
        <TableColumn fx:id="tableColumnName" prefWidth="150" text="%default.name"/>
        <TableColumn fx:id="tableColumnLastname" prefWidth="400" text="%default.lastname"/>
        <TableColumn fx:id="tableColumnNif" prefWidth="150" text="%default.nif"/>
        <TableColumn fx:id="tableColumnEmail" prefWidth="300" text="%default.email"/>
    </columns>
</TableView>
</VBox>
